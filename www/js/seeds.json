categories = [
	{
		name: "DRINKS",
		sub_categories: DRINKS
	},
	{
		name: "FOODS",
		sub_categories: FOODS
	},
	{
		name: "SHEESA AND SNACKS",
		sub_categories: SHEESA_AND_SNACKS
	},
]


categories.each do |cat|
	category = Category.create name: cat[:name]
	cat[:sub_categories].each do |sub_cat|
		sub_category = category.sub_categories.create name: sub_cat[:name]
		sub_cat[:menus].each do |menu|
			sub_category.menus.create({
				name: menu[:name], 
				price: menu[:price], 
				image: "#{BASE_URL}/assets/#{cat[:name].titleize}/#{sub_cat[:name].titleize}/#{menu[:name].titleize}.jpg",
				status: "Available"
			})
		end
	end
end

